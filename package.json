{
  "name": "stylay",
  "version": "0.1.1",
  "description": "Compile your Stylus with ease",
  "main": "app/bundle.main.js",
  "author": "Nathan G <n6g7@gnab.fr>",
  "license": "MIT",
  "scripts": {
    "build": "webpack-dev-server",
    "dev": "NODE_ENV=dev electron .",
    "pretest": "yarn test:format && yarn test:lint",
    "start": "electron .",
    "test": "mocha --compilers js:babel-core/register --require test/helper.js --recursive",
    "test:format": "prettier -l \"src/**/*.js\"",
    "test:lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/n6g7/stylay.git"
  },
  "bugs": {
    "url": "https://github.com/n6g7/stylay/issues"
  },
  "homepage": "https://github.com/n6g7/stylay#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "jade": "^1.11.0",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.7.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "chai-immutable": "^1.6.0",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "electron": "^2.0.7",
    "enzyme": "^2.5.1",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^0.9.0",
    "ignore-styles": "^4.0.0",
    "img-loader": "^1.3.1",
    "mocha": "^2.5.3",
    "prettier": "^1.14.2",
    "react-addons-test-utils": "^15.3.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "Promise": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ]
  },
  "build": {
    "appId": "fr.gnab.stylay",
    "category": "public.app-category.developer-tools"
  }
}
